   "source": [
    "# to remove last (right most) set bit \n",
    "x = 10\n",
    "print(f'binary of {x} is :',binary(x))\n",
    "\n",
    "# in case of EVEN nums only:\n",
    "ans = x & (x-1)\n",
    "print(f'binary of {x-1} is :',binary(x-1))\n",
    "\n",
    "print('Answer is :', binary(ans))\n"
